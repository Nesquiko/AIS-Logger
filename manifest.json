{
    "name": "AISLogger",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["*://is.stuba.sk/auth/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "AIS Logger"
    },
    "permissions": ["storage"]
}
